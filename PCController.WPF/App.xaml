<views:MvxApplication
    x:Class="PCController.WPF.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converters="clr-namespace:PCController.WPF.Converters"
    xmlns:local="clr-namespace:PCController.WPF"
    xmlns:ui="http://schemas.modernwpf.com/2019"
    xmlns:views="clr-namespace:MvvmCross.Platforms.Wpf.Views;assembly=MvvmCross.Platforms.Wpf"
    StartupUri="MainWindow.xaml">


    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ui:ThemeResources />
                <ui:XamlControlsResources />
            </ResourceDictionary.MergedDictionaries>
            <SolidColorBrush x:Key="SimpleStackPanelBackgroundDarkGrey" Color="#FF242424" />
            <converters:EnumValuesConverter x:Key="EnumValuesConverter" />
            <Color x:Key="SolidBlue">#FF225D8B</Color>
            <Color x:Key="SolidRed">#FF8C332D</Color>
            <Color x:Key="SolidGreen">#FF577337</Color>
        </ResourceDictionary>
    </Application.Resources>


</views:MvxApplication>
